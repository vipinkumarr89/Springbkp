<?xml version="1.0" encoding="UTF-8" ?>


<hibernate-mapping>
    <class name="MyPackage.domain.Employee" table="employee">
        <id name="empid" column="empid">
            <generator class="increment"/>
        </id>
        <property name="ename" column="ename"/>
        <property name="basic" column="basic"/>
        <property name="HRA" column="HRA"/>
        <property name="DA" column="DA"/>
        <property name="Deductions" column="Deductions"/>
    </class>
</hibernate-mapping>
